{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testingmigrateoracle"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://onprem123.vault.azure.net/"
		},
		"LS_ADLS2_Staging_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dankdatastorage.dfs.core.windows.net/"
		},
		"SqlServeronprem_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "VIJAYKUMAR"
		},
		"SqlServeronprem_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "bank"
		},
		"SqlServeronprem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vijay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testingmigrateoracle_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLS2_Staging_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS2_Staging_properties_typeProperties_url')]"
					},
					"SqlServeronprem_properties_typeProperties_server": {
						"value": "[parameters('SqlServeronprem_properties_typeProperties_server')]"
					},
					"SqlServeronprem_properties_typeProperties_database": {
						"value": "[parameters('SqlServeronprem_properties_typeProperties_database')]"
					},
					"SqlServeronprem_properties_typeProperties_userName": {
						"value": "[parameters('SqlServeronprem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}